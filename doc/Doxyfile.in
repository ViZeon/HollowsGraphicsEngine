PROJECT_NAME           = "HollowsGraphicsEngine"
OUTPUT_DIRECTORY       = "@CMAKE_CURRENT_BINARY_DIR@/docs"

RECURSIVE              = YES
EXTRACT_ALL            = YES
EXTRACT_PRIVATE        = YES
EXTRACT_STATIC         = YES

GENERATE_HTML          = YES
GENERATE_LATEX         = NO

HAVE_DOT               = YES
DOT_IMAGE_FORMAT       = svg
CLASS_DIAGRAMS         = YES
COLLABORATION_GRAPH    = YES
CALL_GRAPH             = YES
CALLER_GRAPH           = YES

INPUT                  = @CMAKE_SOURCE_DIR@
FILE_PATTERNS          = *.h *.hpp *.cpp *.cxx
